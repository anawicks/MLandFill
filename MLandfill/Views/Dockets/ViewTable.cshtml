
@{
    ViewBag.Title = "ViewTable";
}
<style>

    body {
  padding-top: 40px;
}

.with-margin {
  margin-bottom: 5px;
}

.spacer5 {
  height: 5px;
}
</style>
<h2>ViewTable</h2>
<div class="container">

    <div class="row">

        <div class="col-sm-12">
            <p>Below are two examples of how to create vertical space between Bootstrap rows</p>
        </div>

    </div>

    <div class="row">

        <div class="col-sm-4">
            <h3>Default</h3>
        </div>

        <div class="col-sm-4">
            <h3>Bottom margin on content</h3>
        </div>

        <div class="col-sm-4">
            <h3>Spacer div</h3>
        </div>

    </div>

    <div class="row">

        <div class="col-sm-4">
            <div class="row">
                <div class="col-sm-12">
                    <div class="content">Row 1</div>
                </div>
            </div>
            <div class="row">
                <div class="col-sm-12">
                    <div class="content">Row 2</div>
                </div>
            </div>
        </div>

        <div class="col-sm-4">
            <div class="row">
                <div class="col-sm-12">
                    <div class="content with-margin">Row 3</div>
                </div>
            </div>
            <div class="row">
                <div class="col-sm-12">
                    <div class="content">Row 4</div>
                </div>
            </div>
        </div>

        <div class="col-sm-4">
            <div class="row">
                <div class="col-sm-12">
                    <div class="content">Row 5</div>
                </div>
            </div>
            <div class="spacer5"></div>
            <div class="row">
                <div class="col-sm-12">
                    <div class="content">Row 6</div>
                </div>
            </div>
        </div>

    </div>

</div>


